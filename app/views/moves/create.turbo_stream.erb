<%= turbo_stream.update dom_id(@match) do %>
    <% if @match.winner != nil %>
        <%= render partial: "winning-board", locals: {match: @match } %>
    <% else %>
        <%= render @match %>
    <% end %>
<% end %>

<%= turbo_stream.update "turn-tracker" do %>
 <%= render partial: "turn-tracker", locals: {match: @match} %>
<% end%>